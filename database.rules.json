{
  "rules": {
    ".read": true,
    "photos": {
      ".read": true,
      ".write": "auth != null"
    },
    "users": {
      "$userId": {
        ".read": true,
        "photos": {
          "$photoId": {
            ".read": true,
            "likes": {
              ".read": true,
              ".write": "auth != null"
            }
          }
        }
      }
    }
  }
}
